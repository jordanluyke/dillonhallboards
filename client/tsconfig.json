{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "lib": ["dom", "esnext"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noStrictGenericChecks": true,
        "outDir": "target/src",
        "pretty": true,
        "sourceMap": true,
        "strictNullChecks": false,
        "suppressImplicitAnyIndexErrors": true,
        "target": "esnext",
        "typeRoots": []
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "src/main-aot.ts"
    ]
}
